{
  "extends": [
    "config:recommended",
    ":disableDependencyDashboard"
  ],
  "prHourlyLimit": 0,
  "prConcurrentLimit": 0,
  "packageRules": [
    {
      "matchDatasources": [
        "docker"
      ],
      "matchPackageNames": ["mcr.microsoft.com/dotnet-buildtools/prereqs"],
	    "matchPaths": ["UnitTests.proj"],
	    "versioning": "regex:^\D*-(?<major>\\d+)\\.(?<minor>\\d+)(?:-(?<compatibility>.+)$",
      "enabled": true
    }
  ]
}
