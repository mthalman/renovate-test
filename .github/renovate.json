{
  "extends": [
    "config:recommended",
    ":disableDependencyDashboard"
  ],
  "prHourlyLimit": 0,
  "prConcurrentLimit": 0,
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": ["UnitTests.proj"],
      "matchStrings": [
        "mcr.microsoft.com/dotnet-buildtools/prereqs:\\D+-(?<currentValue>\\d+\\.\\d+)(-\\D+)?"
      ],
      "datasourceTemplate": "docker",
      "depNameTemplate": "mcr.microsoft.com/dotnet-buildtools/prereqs",
      "versioningTemplate": "semver"
    },
    {
      "customType": "regex",
      "fileMatch": ["versions.json"],
      "matchStrings": [
        "\"omnisharp\":\\s*\"(?<currentValue>[^\"]+)\""
      ],
      "datasourceTemplate": "github-releases",
      "depNameTemplate": "OmniSharp/omnisharp-roslyn",
      "versioningTemplate": "semver"
    }
  ]
}
